    "abit": "hardhat run scripts/merge_abi.ts --network zksync_testnet",
    "runst_prigo": "hardhat run scripts/runs_testnet_prigo.ts --network zksync_testnet",
    "runst_prigo2": "hardhat run scripts/runs_testnet_prigo2.ts --network scroll_sepolia",

    "DEPLOY_ALL": "yarn rm-test && yarn deploy-test && yarn setupt && yarn abit && yarn runst_prigo",
    "DEPLOY_ZK_TEST": "yarn rm-test && hardhat --network zksync_testnet  deploy_zk_test",


