    "DEPLOY_BASE_TEST": "hardhat --network base_testnet deployScripts",
    "DEPLOY_CONFLUX_TEST": "hardhat --network conflux_test deployScripts",
    "DEPLOY_OPT_TEST": "hardhat --network optimism_goerli deployScripts",

    "DEPLOY_ARB_TEST": "hardhat --network arbitrum_goerli  deployScripts",
    "DEPLOY_FUJI_TEST": "hardhat --network fuji deployScripts",
    "DEPLOY_BSC_TEST": "hardhat --network bsc_testnet deployScripts",
    "DEPLOY_LINEA_TEST": "hardhat --network linea_testnet deployScripts",
    "DEPLOY_MUMBAI_TEST": "hardhat --network mumbai deployScripts",

    "DEPLOY_SCROLL_SEPOLIA_TEST": "hardhat --network scroll_sepolia deployScripts",
    "DEPLOY_SCROLL_SEPOLIA_AFTER_PYTH": "hardhat --network scroll_sepolia run ./scripts/runs_testnet_buy_lp2.ts ",


    "DEPLOY_MANTLE_TEST": "hardhat --network mantle_testnet deployScripts",
    "DEPLOY_BASE_MAIN": "hardhat --network base_mainnet deployScripts",
    "DEPLOY_SCROLL_MAIN": "hardhat --network scroll_mainnet deployScripts",
    "DEPLOY_MANTLE_MAIN": "hardhat --network mantle_mainnet deployScripts",
