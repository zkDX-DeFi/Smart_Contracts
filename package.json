    "deploy_linea_mainnet": "hardhat --network linea_mainnet deployScripts",
    "contract_settings_linea_mainet": "hardhat --network linea_mainnet  contract_settings",

    "view": "hardhat run scripts/view.ts --network zksync_testnet",
    "ops": "hardhat run scripts/ops.ts --network zksync_testnet",
    "ods": "hardhat run scripts/ods.ts --network zksync_testnet",

